function bai3_6(n)
index = 1;
F1 = 1; F2 = 1;
fprintf('(%d)', F1);
fprintf('(%d)', F2);
while (index <= n-2)
    F = F2 + F1;
    F1 = F2;
    F2 = F;
    fprintf('%d\n', F2/F1);
    fprintf('(%d)', F);
    n = n + 1;
end
fprintf('Ta thay rang ti le Fn/F(n - 1) tien dan den (1 + sqrt(5))/2\n'); 
end